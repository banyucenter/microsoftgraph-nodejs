<h1>Calendar</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">name</th>
      <th scope="col">color</th>
    </tr>
  </thead>
  <tbody>
    {{#each calends}}
      <tr>
        <td>{{this.name}}</td>
        <td>{{this.color}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
